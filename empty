		if L_171_forvar1:IsA("BasePart") then
			L_171_forvar1:Destroy()
		end
	end
    end)

farmtab:Toggle("Auto Chests Money Farm", function(State)
getgenv().Money = State
game:GetService("RunService").Stepped:connect(
    function()
        if getgenv().Money then
           game.Players.LocalPlayer.Character:WaitForChild('Humanoid'):ChangeState(11)
        end
    end
)

         
while getgenv().Money do
    wait()
    pcall(function()
            if getgenv().Money then
               local Time =
                (GetClosestt().CFrame.p + Vector3.new(0, 0, 0) -
                game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude / getgenv().speed
            local Info = TweenInfo.new(Time, Enum.EasingStyle.Linear)
            local Tween =
                game:GetService("TweenService"):Create(
                game.Players.LocalPlayer.Character.HumanoidRootPart,
                Info,
                {
                    CFrame = CFrame.new(
                        GetClosestt().CFrame.p + Vector3.new(0,  0, 0)
                    )
                }
            )
            Tween:Play()
        end 
        end
    )
end
end)


