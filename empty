local function GetClosestt()
    local TargetDistance = math.huge
    local Target
    for i, v in ipairs(game:GetService("Workspace"):GetChildren()) do
        if v.Name == "Chest1" or v.Name == "Chest2" or v.Name == "Chest3" then
            local Mag = (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Position).magnitude
            if Mag < TargetDistance then
                TargetDistance = Mag
                Target = v
            end
        end
    end
    return Target
end
game:GetService('RunService').Stepped:connect(function()
    ClosestPlayer = GetClosestToCuror()
    if ClosestPlayer then
     workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.p,ClosestPlayer.Character.HumanoidRootPart.CFrame.p)
    end 
end)



if game.PlaceId == 2753915549 then
game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("SetTeam","Pirates")
end
repeat wait() until game.Players.LocalPlayer.Backpack:FindFirstChild("Combat")
for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetDescendants()) do
if v.ClassName == "Tool" then
v.Parent = game.Players.LocalPlayer.Backpack
end end 

tools ={}
for i,v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetDescendants()) do
if v.ClassName == "Tool" then
table.insert(tools,v.Name)
end end 

mobs = {}
for i,v in pairs(game:GetService("ReplicatedStorage"):GetDescendants()) do
if v.ClassName == "Model"  and v:FindFirstChild("Stun") then
table.insert(mobs,v.Name)
table.sort(mobs)
end end 
for i,v in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do
if v.ClassName == "Model"  and v:FindFirstChild("Stun") then
table.insert(mobs,v.Name)
table.sort(mobs)
end end  
getgenv().speed = 100

farmtab:Toggle("Auto Mob Farm",function(State)
    farm = State
    game:GetService("RunService").Stepped:connect(function()
            if farm then
               game.Players.LocalPlayer.Character:WaitForChild('Humanoid'):ChangeState(11)
            end
        end
    )
    spawn(
        function()
            game:GetService("RunService").Stepped:connect(
                function()
                    pcall(
                        function()
                            if farm then
                                for i, v in pairs(game:GetService("ReplicatedStorage")[mobs]:GetDescendants()) do
                                    if v:IsA("MeshPart") and v.Name ~= "UpperTorso" then
                                        local Time =
                                            (v.CFrame.p + Vector3.new(0, 0, 0) -
                                            game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude /
                                            getgenv().speed
                                        local Info = TweenInfo.new(Time, Enum.EasingStyle.Linear)
                                        local Tween =
                                            game:GetService("TweenService"):Create(
                                            game.Players.LocalPlayer.Character.HumanoidRootPart,
                                            Info,
                                            {
                                                CFrame = CFrame.new(v.CFrame.p + Vector3.new(0, 0, -3))
                                            }
                                        )
                                        Tween:Play()
                                    end
                                end
                            end
                        end
                    )
                end
            )
        end
    )
    
    
    spawn(
        function()
            if farm then
                local Character = game:GetService("Players").LocalPlayer.Character
                local Clone = Character.LowerTorso.Root:Clone()
                Character.LowerTorso.Root:Destroy()
                Clone.Parent = Character.LowerTorso
            end
        end
    )
    spawn(
        function()
            game.Players.LocalPlayer.CharacterAdded:Connect(
                function()
                    if farm then
                        wait(3)
                        local Character = game:GetService("Players").LocalPlayer.Character
                        local Clone = Character.LowerTorso.Root:Clone()
                        Character.LowerTorso.Root:Destroy()
                        Clone.Parent = Character.LowerTorso
                    end
                end
            )
        end
    )
    
    spawn(
        function()
            while farm do
                wait()
                pcall(
                    function()
                        for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                            if v.ClassName == "Tool" and v.Name == tools then
                                v.Parent = game.Players.LocalPlayer.Character
                                wait(5)
                                v.Parent = game.Players.LocalPlayer.Backpack
                            end
                        end
                    end
                )
            end
        end
    )
    
    spawn(
        function()
          while wait() do
                    pcall(
                        function()
                            if farm then
                                game.Workspace.Gravity = 0
                                game:GetService("VirtualUser"):ClickButton1(Vector2.new(9e9, 9e9))
                                for i, v in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do
                                    if v:IsA("Model") and v.Name == mobs then
                                        local Time =
                                            (v.HumanoidRootPart.CFrame.p + Vector3.new(0, -5, 3) -
                                            game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude /
                                            getgenv().speed
                                        local Info = TweenInfo.new(Time, Enum.EasingStyle.Linear)
                                        local Tween =
                                            game:GetService("TweenService"):Create(
                                            game.Players.LocalPlayer.Character.HumanoidRootPart,
                                            Info,
                                            {
                                                CFrame = CFrame.new(v.HumanoidRootPart.CFrame.p + Vector3.new(0, -5, 3))
                                            }
                                        )
                                        Tween:Play()
                                        if not farm then
                                        game.Workspace.Gravity = 196.2
                                        end
                                    end
                                end
                            end
                        end
                    )
                end
        end
    )
    
    
    end)

    repeat wait() until game.Players.LocalPlayer.Character
    farmtab:Dropdown("Select Tool", tools, function(String)
    tools = String
    end)

    farmtab:Dropdown("select Mob to farm", mobs, function(String)
        mobs = String
        end)


farmtab:Slider("Teleport Speed", 0,250,0, function(Value)
    getgenv().speed = Value
end)

